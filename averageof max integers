#Your code below
r,c=map(int,input().split())
l,rs,cs=[],0,0
for i in range(r):
    a=list(map(int,input().split()))
    l.append(a)
    rs+=max(a)
print('%.2f'%(rs/r))
for i in range(c):
    m=0
    for j in range(r):
        if l[j][i]>m:
            m=l[j][i]
    cs+=m
print('%.2f'%(cs/c))
        
    
