n,k=map(int,input().split())
count=0
for i in range(1,k+1):
    if count<n:
        print(i,end=" ")
        count+=1
for i in range(n):
    for j in range(k-1,0,-1):
        if count<n:
            print(j,end=" ")
            count+=1
    for j in range(2,k+1):
        if count<n:
            print(j,end=" ")
            count+=1
